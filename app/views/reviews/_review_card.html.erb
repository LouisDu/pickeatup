<div class="thumbnail">
  <%= image_tag('http://unsplash.it/300/200', class: 'img-responsive') %>
  <div class="caption">
    <h3><%= review.user.profile.first_name %></h3>
    <p><%= review.user.profile.last_name %></p>
    <hr>
    <p><strong>Note</strong><%= review.rating %>/5</p>
    <p><strong>Description: </strong><%= review.content %></p>
  </div>
</div>
